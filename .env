# Configuración del servidor
PORT=3001
NODE_ENV=development

# CORS
CORS_ORIGIN=*

# API de Go
GO_API_URL=http://project_goland:3000

# JWT (Opcional)
JWT_SECRET=your-super-secret-key-change-this-in-production
JWT_EXPIRES_IN=24h

# Límites
MAX_MATRIX_SIZE=1000
MAX_REQUEST_SIZE=10mb

# Logging
LOG_LEVEL=info
LOG_FORMAT=combined
